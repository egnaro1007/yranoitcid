<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="mothership" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="900.0" prefWidth="1440.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yranoitcid.frontend.MothershipController">
   <children>
      <VBox prefWidth="300.0" HBox.hgrow="ALWAYS">
         <children>
            <ImageView fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@../image/agdhhdhdjh.jpg" />
               </image>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin></ImageView>
            <Button fx:id="toDictionary" maxWidth="1.7976931348623157E308" minWidth="300.0" mnemonicParsing="false" onAction="#switchToDictionary" prefHeight="50.0" text="Dictionary" VBox.vgrow="ALWAYS" />
            <Button fx:id="toTranslator" maxWidth="1.7976931348623157E308" minWidth="300.0" mnemonicParsing="false" onAction="#switchToTranslator" prefHeight="50.0" text="Online Translator" VBox.vgrow="ALWAYS" />
            <Button fx:id="toEditor" maxWidth="1.7976931348623157E308" minWidth="300.0" mnemonicParsing="false" onAction="#switchToEditor" prefHeight="50.0" text="Managerment" VBox.vgrow="ALWAYS" />
            <Button fx:id="toWordChain" maxWidth="1.7976931348623157E308" minWidth="300.0" mnemonicParsing="false" onAction="#switchToWordChain" prefHeight="50.0" text="Game" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <VBox fx:id="mainPane" prefWidth="1140.0" HBox.hgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="270.0" text="WELCOME" textAlignment="CENTER" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria" size="50.0" />
               </font>
            </Label>
            <Label fx:id="time" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="50.0" text="time" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="date" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="50.0" text="date" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
         </children></VBox>
   </children>
</HBox>
