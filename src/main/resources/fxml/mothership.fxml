                  </StackPane>
                  <Label text="Reload CSS" alignment="CENTER" prefHeight="${reloadCSSContainer.height}" prefWidth="${reloadCSSContainer.width}" mouseTransparent="true">
                    <styleClass>
                      <String fx:value="side-bar-button" />
                      <String fx:value="transparent-background" />
                    </styleClass>
                  </Label>
               </children>
            </StackPane>
         </children>
