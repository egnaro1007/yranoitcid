<!--            <Button fx:id="toDictionaryButton" maxWidth="Infinity" minWidth="${leftSideBar.minWidth}" mnemonicParsing="false" onAction="#switchToDictionary" prefHeight="50.0" text="Dictionary" VBox.vgrow="ALWAYS" /> -->
